use strict;

use WakeUpManager::WWW;

#
# WWW lib
my $www = WakeUpManager::WWW->new (
	ajax_mode => 1,
	ajax_page_name => 'HostState',
	ajax_func_name => 'get_host_state',
);
if (! $www) {
	print "Content-Type: text/html\n\nAn internal error has occured. Please contact the system administrator.\n";
	exit 0;
}

print $www->get_page ();
